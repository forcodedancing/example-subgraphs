{
  "name": "tx-send-token",
  "version": "0.1.0",
  "repository": "https://github.com/graphprotocol/example-subgraph",
  "license": "MIT",
  "scripts": {
    "prepare:blog": "mustache config/blog.json subgraph.template.yaml > subgraph.yaml",
    "prepare:cosmoshub": "mustache config/cosmoshub.json subgraph.template.yaml > subgraph.yaml",
    "prepare:osmosis": "mustache config/osmosis.json subgraph.template.yaml > subgraph.yaml",
    "codegen": "graph codegen",
    "build": "graph build",
    "create-local": "graph create tx-send-token --node http://127.0.0.1:8020",
    "deploy-local": "graph deploy tx-send-token -l v0.1.0 --ipfs http://127.0.0.1:5001 --node http://127.0.0.1:8020",
    "remove-local": "graph remove tx-send-token --node http://127.0.0.1:8020",
    "create": "graph create tx-send-token --node https://api.thegraph.com/deploy/",
    "deploy": "graph deploy tx-send-token --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.30.0",
    "@graphprotocol/graph-ts": "^0.27.0",
    "mustache": "^4.2.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "as-proto": "^0.2.3"
  }
}
